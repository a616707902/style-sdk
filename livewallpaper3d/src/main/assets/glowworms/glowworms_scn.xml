<?xml version="1.0" encoding="UTF-8"?>
<Scene>
    
    <SceneObjects></SceneObjects>
    
    <RandomObjectsGroups>
        
        <RandomObjectsGroup
          id="glowworms"
          intensity="3"
          enabled="false">
            
            <Configurator name="Basic">
                <Params>
                    <SpeedFrom>-25, 90, 0</SpeedFrom>
                    <SpeedTo>25, 150, 0</SpeedTo>
                    <PositionFrom>-800, -600, -500</PositionFrom>
                    <PositionTo>800, -600, -500</PositionTo>
                    <ScalingFrom>75</ScalingFrom>
                    <ScalingTo>125</ScalingTo>
                    <AirResistanceFrom>0.001</AirResistanceFrom>
                    <AirResistanceTo>0.0015</AirResistanceTo>
                    <WeightFrom>-1</WeightFrom>
                    <ZIndexFrom>5000</ZIndexFrom>
                </Params>
            </Configurator>
            
            <Behaviour name="Move"></Behaviour>
            <Behaviour name="AirResistance"></Behaviour>
            <Behaviour name="Gravity"/>
            <Behaviour name="RemoveOnOutOfBox">
                <Params>
                    <p1>-1000,-1000,-1000</p1>
                    <p2>1000, 1000, 1000</p2>
                </Params>
            </Behaviour>
            <Behaviour name="Scaling">
                <Params>
                    <ScalingFrom>0.6, 0.6, 1</ScalingFrom>
                    <ScalingTo>1, 1, 1</ScalingTo>
                    <Period>1500</Period>
                </Params>
            </Behaviour>
            
            <PrototypeId>glowworm.1</PrototypeId>
            <PrototypeId>glowworm.2</PrototypeId>
            <PrototypeId>glowworm.3</PrototypeId>
            <PrototypeId>glowworm.4</PrototypeId>
            <PrototypeId>glowworm.5</PrototypeId>
            <PrototypeId>glowworm.6</PrototypeId>
            <PrototypeId>glowworm.7</PrototypeId>
            
        </RandomObjectsGroup>
        
    </RandomObjectsGroups>
    
</Scene>
